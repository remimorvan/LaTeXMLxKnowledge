package LaTeXML::Package::Pool;
use strict;
use warnings;
use LaTeXML::Package;
use feature 'state';

DefPrimitive('\knowledge{}{}',
  sub { my $x = $_[1]->toString(); Note("knowledge: replacement > $x"); return; }
);

DefConstructor('\kl[]{}',
  sub { my $x = $_[2]->toString(); Note("kl: replacement > $x"); return; }
);

1; 